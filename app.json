{
	"name": "wbot-md",
	"description": "A WhatsApp Bot built with Baileys",
	"buildpacks": [
		{
			"url": "heroku/nodejs"
		},
		{
			"url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
		},
		{
			"url": "https://github.com/jayshreeanand/heroku-buildpack-libwebp.git"
		}
	],
	"env": {
		"botName": {
			"description": "Your bot name",
			"value": "SMH BOT"
		},
		"owner": {
			"description": "Your Phonenumber[]",
			"value": ["62xx@s.whatsapp.net"]
		},
		"openWeather": {
			"description": "Open Wheater Apikey",
			"value": ""
		},
		"igCookie": {
			"description": "Instagram Cooke",
			"value": ""
		},
		"session": {
			"description": "Name MD Session",
			"value": "session-md.json"
		},
		"user_db": {
			"description": "Name User Database",
			"value": "user_db.json"
		},
		"timezone": {
			"description": "Your Timezone",
			"value": "Asia/Jakarta"
		},
		"footer": {
			"description": "Footer for some message",
			"value": "SMH BOT"
		}
	}
